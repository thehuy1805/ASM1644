<h3>
    <p>Orders List</p>
    <a href="/product" class="btn btn-primary">
        <i class="far fa-calendar-plus"></i> New Product
    </a>

    <a href="/orders/list" class="btn btn-success">
        <i class="fas fa-stream"></i> List Order
    </a>
</h3>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Quantity</th>
            <th>Amount</th>
            <th>Brand</th>
            <th>Employee</th>
            <th>OrderDate</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.Quantity}}</td>
            <td>{{this.Amount}}</td>
            <td>{{this.Brand}}</td>
            <td>{{this.Employee}}</td>
            <td>{{this.OrderDate}}</td>

        </tr>
        {{/each}}
    </tbody>
</table>